
# ARM Compile Option 
    
INCLUDEDIRS += 
#header 파일이 어디있는지
    
CC = gcc
#컴파일러의 위치
    
CFLAGS   += $(INCLUDEDIRS)
CFLAGS   += -Wall -O2

LDFLAGS +=
#라이브러리의 위치

#
# Compilation target for C files
#
%.o:%.c
	@echo "Compiling $< ..."
	$(CC) -c $(CFLAGS) -o $@ $<


TARGET = ts-pc

OBJS =AcceptTCPConnection.o DieWithError.o HandleTCPClient.o TCPEchoServer.o  parson.o #목적파일 이름
SRCS =AcceptTCPConnection.c DieWithError.c HandleTCPClient.c TCPEchoServer.c parson.c  #소스파일 이름


$(TARGET) : $(OBJS) 
	$(CC) $(CFLAGS) $(LDFLAGS) $(OBJS)  -o $@ 

dep :
	mkdep $(INCLUDEDIRS) $(SRCS)

clean :	
	rm -rf $(OBJS) $(TARGET) core

